while true do
	swait()
	if not workspace:FindFirstChild(Char.Name) then print("GOD DAMN.") break end
	sine = sine + 1
	Animator.Parent = nil
	Animate.Disabled = true
	CHECKHELL("FLAMES")
	uhsuhs.Text = string.upper(GLITCHER)
	uhsuhs.Rotation=5*sin(sine/43) 
	sushi.Rotation=-5*sin(sine/43) 

	if mode == 6 then
		MAINCOLOR=BrickColor.Random().Color
	elseif mode == 9 then
		MAINCOLOR=Color3.fromRGB(0,0,155+100*math.sin(sine/24))
	elseif mode == "inf" then
		MAINCOLOR=Color3.fromRGB(155+100*math.sin(sine/45),155-100*math.sin(sine/45),0)
	elseif mode=="nil" then
		ran=math.random(0,80)
		MAINCOLOR=Color3.fromRGB(ran,ran,ran)
	elseif mode==14 then
		MAINCOLOR=Color3.fromRGB(0,0,song.PlaybackLoudness)
	elseif mode=="corrupt." then
		MAINCOLOR=Color3.fromRGB(10*sin(sine/65),10*sin(sine/65),10*sin(sine/65))
	elseif mode=="corrupted." then
		MAINCOLOR=Color3.fromRGB(10*sin(sine/65),10*sin(sine/65),10*sin(sine/65))
	end
	eFrame.BorderColor3 = MAINCOLOR
	TextLabel.TextStrokeColor3 = MAINCOLOR
	TextLabel_2.TextStrokeColor3 = MAINCOLOR
	
	TextLabel.Text=NAME
	TextLabel_2.Text=thingie
	-- UI SETUP
