--[[
gui.Music.Bar.Size = UDim2.new(theme.TimePosition/(theme.TimeLength-offset),0,1,0)
gui.Music.CurrentTime.Text = transform(theme.TimePosition)
gui.Music.Length.Text = transform(theme.TimeLength-offset)

]]
function transform(seconds)
seconds = math.floor(seconds+.5)
local minutes,extra = 0,""

if seconds >= 60 then
repeat
if seconds >= 60 then
seconds = seconds - 60
minutes = minutes + 1
end
until seconds < 60
end
if seconds < 10 then
extra = "0"
end
return(minutes..":"..extra..seconds)
end

spawn(function()
	while true do swait()
		if not SWITCHINGANIM then
		if mode=="corrupt." then
			if math.random(1,58) == 1 then
			song.PlaybackSpeed = -0.03+math.random(4505,5765)/5906
			wait(1)
			end
		elseif mode=="corrupted." then
			if math.random(1,24) == 1 then
			song.PlaybackSpeed = -0.03+math.random(4404,5765)/5906
			wait(1)
			end
		else
			song.PlaybackSpeed=1
		end
		end
	end
end)
