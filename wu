function PixelBlock(bonuspeed,FastSpeed,type,pos,x1,y1,z1,value,color,outerpos,heart,invert)
if not getfenv().run then return end
local type = type
local rng = Instance.new("Part", Char)
        rng.Anchored = true
        rng.Color = color
        rng.CanCollide = false
        rng.FormFactor = 3
        rng.Name = "Ring"
        rng.Material = "Neon"
        rng.Size = Vector3.new(1, 1, 1)
        rng.Transparency = 0
        rng.TopSurface = 0
        rng.BottomSurface = 0
        rng.CFrame = pos
rng.CFrame = rng.CFrame + rng.CFrame.lookVector*outerpos
        local rngm = Instance.new("SpecialMesh", rng)
        rngm.MeshType = "Brick"

rngm.Scale = Vector3.new(x1,y1,z1)

local scaler2 = 1
local speeder = FastSpeed/10
if type == "Add" then
scaler2 = 1*value
elseif type == "Divide" then
scaler2 = 1/value
end
coroutine.resume(coroutine.create(function()
for i = 0,10/bonuspeed,0.1 do
swait()

if type == "Add" then
scaler2 = scaler2 - 0.01*value/bonuspeed
elseif type == "Divide" then
scaler2 = scaler2 - 0.01/value*bonuspeed
end


speeder = speeder - 0.01*FastSpeed*bonuspeed/10
if(invert)then rng.CFrame = rng.CFrame - rng.CFrame.lookVector*speeder*bonuspeed else rng.CFrame = rng.CFrame + rng.CFrame.lookVector*speeder*bonuspeed end
--rng.Transparency = rng.Transparency + 0.01*bonuspeed
rngm.Scale = rngm.Scale - Vector3.new(scaler2*bonuspeed, scaler2*bonuspeed, scaler2*bonuspeed)
end
rng:Destroy()
end))
end
function bigblock(bonuspeed,FastSpeed,type,pos,x1,y1,z1,value,color,outerpos,heart,invert)
if not getfenv().run then return end
local type = type
local rng = Instance.new("Part", Char)
        rng.Anchored = true
        rng.Color = color
        rng.CanCollide = false
        rng.FormFactor = 3
        rng.Name = "Ring"
        rng.Material = "Neon"
        rng.Size = Vector3.new(1, 1, 1)
        rng.Transparency = 0.5
        rng.TopSurface = 0
        rng.BottomSurface = 0
        rng.CFrame = pos
rng.CFrame = rng.CFrame-- + rng.CFrame.lookVector*outerpos
        local rngm = Instance.new("SpecialMesh", rng)
        rngm.MeshType = "Sphere"

rngm.Scale = Vector3.new(x1,y1,z1)

local scaler2 = 1
local speeder = FastSpeed/10
if type == "Add" then
scaler2 = 1*value
elseif type == "Divide" then
scaler2 = 1/value
end
coroutine.resume(coroutine.create(function()
for i = 0,10/bonuspeed,0.1 do
swait()

if type == "Add" then
scaler2 = scaler2 - 0.01*value/bonuspeed
elseif type == "Divide" then
scaler2 = scaler2 - 0.01/value*bonuspeed
end


speeder = speeder - 0.01*FastSpeed*bonuspeed/10
--if(invert)then rng.CFrame = rng.CFrame - rng.CFrame.lookVector*speeder*bonuspeed else rng.CFrame = rng.CFrame + rng.CFrame.lookVector*speeder*bonuspeed end
rng.Transparency = rng.Transparency + 0.01*bonuspeed
rngm.Scale = rngm.Scale + Vector3.new(scaler2*bonuspeed, scaler2*bonuspeed, scaler2*bonuspeed)
end
rng:Destroy()
end))
end

function sfx(id,volume)
	local sound=Instance.new("Sound",Char)
	sound.PlayOnRemove=true
	sound.Volume=volume
	sound.SoundId="rbxassetid://"..id
	--sound.PlaybackSpeed=pitch
	sound:Remove()
end

function AddMode(Keybind,Name,Number,Walk,tab,FUNNNYYY,color,g)
	Mouse.KeyDown:connect(function(e)
	if e~=Keybind or Number==mode or g~=GLITCHER then return end
	for i = 0,2,0.05 do
		swait()
		song.PlaybackSpeed=1-i/4
		SWITCHINGANIM=true
		funanim="switching"
		ws=0
	end
	SWITCHINGANIM=false
	mode=Number
	walk=Walk
	thingie=FUNNNYYY or thingie
	MAINCOLOR=color or Color3.fromRGB(0,0,0)
	NAME=Name
	song.PlaybackSpeed=1
	song.SoundId=getcustomasset("k/"..Name..".mp3")
	for i = 0, 65 do
	PixelBlock(1,math.random(1,20),"Add",Torso.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),2,2,2,0.04,MAINCOLOR,0)
	end
	for i = 0, 1 do
	bigblock(1,math.random(11,42),"Add",Torso.CFrame,12,12,12,0.04,MAINCOLOR,0)
	end
	sfx(847061203,5)
	sfx(239000203,1.5)
	sfx(579687077,1.5)
	for i = 0,2,0.05 do
		swait()
		SWITCHINGANIM=true
		funanim="done"
		ws=0
	end
	if walk==true then
		change=tab[1]
		movement=tab[2]
		ws=tab[3]
	else
		ws=tab[1]
	end

	reanim['ToxicLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['RainbowGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['DemonLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['AuroraSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	reanim['DemonGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	reanim['VoidLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	SWITCHINGANIM=false
	end)
end

function AddSpecialMode(mainnum,Name,Number,Walk,tab,FUNNNYYY,color,g)
	Mouse.KeyDown:connect(function(e)
	if e~="m" or mainnum~=mode or g~=GLITCHER then return end

	mode=Number
	thingie=FUNNNYYY or thingie
	walk=Walk
	NAME=Name
	MAINCOLOR=color or MAINCOLOR
	song.SoundId=getcustomasset("k/"..Name..".mp3")
	sfx(847061203,5)
	if walk==true then
		change=tab[1]
		movement=tab[2]
		ws=tab[3]
	else
		ws=tab[1]
	end
	for i = 0, 49 do
	PixelBlock(1,math.random(1,20),"Add",Torso.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),2,2,2,0.04,MAINCOLOR,0)
	end
	reanim['ToxicLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['RainbowGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['DemonLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
	reanim['AuroraSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	reanim['DemonGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	reanim['VoidLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
	end)
end

function CHECKHELL(Name)
	if FeminineOffense["="] and FeminineOffense["-"] then
		if mode=="FLAMING SOUL" or GLITCHER~="username" then return end
		mode="FLAMING SOUL"
		walk=false
		ws=400
		NAME=Name
		thingie="PREPARE FOR YOUR DEMISE..."
		MAINCOLOR=Color3.fromRGB(255,0,0)
		reanim['ToxicLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
		reanim['RainbowGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
		reanim['DemonLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"] 
		reanim['AuroraSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
		reanim['DemonGodSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
		reanim['VoidLordSword'].Handle.AccessoryWeld.Part1=Char["Torso"]
		song.SoundId=getcustomasset("k/"..Name..".mp3")
			sfx(847061203,5)
	sfx(239000203,1.5)
	sfx(579687077,1.5)
				for i = 0, 49 do
	PixelBlock(1,math.random(1,20),"Add",Torso.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),2,2,2,0.04,MAINCOLOR,0)
	end
	for i = 0, 1 do
	bigblock(1,math.random(11,42),"Add",Torso.CFrame,34,34,34,0.04,MAINCOLOR,0)
	end
	end
end

