			end
		else
			if funanim=="switching" then
				Alpha=.2
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,height,0)*cf.a(rd(-35-5*sin(sine/.31)),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(-1,-0.16,-0.8)*cf.a(rd(170-5*sin(sine/.4)),rd(30),rd(-5)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(1,-0.16,-0.8)*cf.a(rd(170-5*sin(sine/.4)),rd(-30),rd(5)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,-0.45)*cf.a(rd((-30+35)+5*sin(sine/.31)),rd(-10),rd(3)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,-0.3)*cf.a(rd((35)+5*sin(sine/.31)),rd(10),rd(-3)),Alpha)
			elseif funanim=="done" then
			Alpha=.1
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,height,0)*cf.a(rd(20),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(90)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(-90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,-0.3)*cf.a(rd(-20),rd(0),rd(10)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,-0.3)*cf.a(rd(-20),rd(0),rd(-10)),Alpha)
				
			end
		end
		end
		--[[
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,0.1,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
		]]--
	end
	if (AnimState == "Walk") then
		local Alpha = .1
		wsVal = 4
		-- Sakura Blade Walk

		if attack==false then
			if walk and mode~="alsoo3" and mode~=20 then
			welds[2].C0 = welds[2].C0:lerp(CFrame.new(0, -0.05 + change / 2 * math.cos(sine / 2), 0) * CFrame.Angles(math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * lookvel, torsorotY2 + math.rad(0 - 4 * math.sin(sine / 4)), math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * rightvel + torsorotY2 + math.rad(0 - 1 * math.cos(sine / 4))), 0.1)		
			welds[1].C0 = welds[1].C0:lerp(CFrame.new(0, 1.5, -0.1 - 0.1 * lookvel) * CFrame.Angles(0, torsorotY, math.rad(0) + torsorotY), 0.4)
			welds[1].C1 = welds[1].C1:lerp(CFrame.Angles((change / 10 * math.cos(sine / 2) + 0.1) * lookvel ^ 2, -(change / 10) * math.cos(sine / 4) - torsorotY / 5, change / 5 * math.cos(sine / 4)), 0.1)
			
			welds[4].C0 = welds[4].C0:lerp(CFrame.new(1.5, 0.5 + 0.12 * lookvel ^ 2 - movement / 40 * math.cos(sine / 4) / 3, movement / 150 + movement / 40 * math.cos(sine / 4)) * CFrame.Angles((math.rad(-5 - movement * 2 * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 - movement * 2 * math.cos(sine / 4)), -math.rad(movement * 1 * 0.2 - movement * 1 * math.cos(sine / 2) * lookvel) + math.rad(-5 * rightvel)), 0.2)
			welds[6].C0 = welds[6].C0:lerp(CFrame.new(-1.5, 0.5 + 0.12 * lookvel ^ 2 + movement / 40 * math.cos(sine / 4) / 3, movement / 150 - movement / 40 * math.cos(sine / 4)) * CFrame.Angles((math.rad(-5 + movement * 2 * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 - movement * 2 * math.cos(sine / 4)), -math.rad(-(movement * 1) * 0.2 + movement * 1 * math.cos(sine / 2) * lookvel) + math.rad(-5 * rightvel)), 0.2)
			welds[5].C0 = welds[5].C0:lerp(CFrame.new(-0.5, -0.85 - movement / 15 * math.cos(sine / 4) / 2, (-0.1 + movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			welds[3].C0 = welds[3].C0:lerp(CFrame.new(0.5, -0.85 + movement / 15 * math.cos(sine / 4) / 2, (-0.1 - movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			elseif walk==true and mode=="alsoo3" then
			welds[2].C0 = welds[2].C0:lerp(CFrame.new(0, -0.05 + change / 2 * math.cos(sine / 2), 0) * CFrame.Angles(math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * lookvel, torsorotY2 + math.rad(0 - 4 * math.sin(sine / 4)), math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * rightvel + torsorotY2 + math.rad(0 - 1 * math.cos(sine / 4))), 0.1)		
			welds[1].C0 = welds[1].C0:lerp(CFrame.new(0, 1.5, -0.1 - 0.1 * lookvel) * CFrame.Angles(0, torsorotY, math.rad(0) + torsorotY), 0.4)
			welds[1].C1 = welds[1].C1:lerp(CFrame.Angles((change / 10 * math.cos(sine / 2) + 0.1) * lookvel ^ 2, -(change / 10) * math.cos(sine / 4) - torsorotY / 5, change / 5 * math.cos(sine / 4)), 0.1)
			
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0.3*cos(sine/4),-0.7)*cf.a(rd(120+8*sin(sine/4)),rd(0),rd(20*sin(sine/4/2))),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0.3*cos(sine/4),-0.7)*cf.a(rd(120-8*sin(sine/4)),rd(0),rd(20*sin(sine/4/2))),Alpha)
			welds[5].C0 = welds[5].C0:lerp(CFrame.new(-0.5, -0.85 - movement / 15 * math.cos(sine / 4) / 2, (-0.1 + movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			welds[3].C0 = welds[3].C0:lerp(CFrame.new(0.5, -0.85 + movement / 15 * math.cos(sine / 4) / 2, (-0.1 - movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			elseif walk==true and mode==20 then
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0.13*cos(sine/8),0)*cf.a(rd(0),rd(-30),rd(20+3*sin(sine/4))),Alpha)
			
			welds[2].C0 = welds[2].C0:lerp(CFrame.new(0, -0.05 + change / 2 * math.cos(sine / 2), 0) * CFrame.Angles(math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * lookvel, torsorotY2 + math.rad(0 - 4 * math.sin(sine / 4)), math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * rightvel + torsorotY2 + math.rad(0 - 1 * math.cos(sine / 4))), 0.1)		
			welds[1].C0 = welds[1].C0:lerp(CFrame.new(0, 1.5, -0.1 - 0.1 * lookvel) * CFrame.Angles(0, torsorotY, math.rad(0) + torsorotY), 0.4)
			welds[1].C1 = welds[1].C1:lerp(CFrame.Angles((change / 10 * math.cos(sine / 2) + 0.1) * lookvel ^ 2, -(change / 10) * math.cos(sine / 4) - torsorotY / 5, change / 5 * math.cos(sine / 4)), 0.1)
			
			welds[6].C0 = welds[6].C0:lerp(CFrame.new(-1.5, 0.5 + 0.12 * lookvel ^ 2 + movement / 40 * math.cos(sine / 4) / 3, movement / 150 - movement / 40 * math.cos(sine / 4)) * CFrame.Angles((math.rad(-5 + movement * 2 * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 - movement * 2 * math.cos(sine / 4)), -math.rad(-(movement * 1) * 0.2 + movement * 1 * math.cos(sine / 2) * lookvel) + math.rad(-5 * rightvel)), 0.2)
			welds[5].C0 = welds[5].C0:lerp(CFrame.new(-0.5, -0.85 - movement / 15 * math.cos(sine / 4) / 2, (-0.1 + movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			welds[3].C0 = welds[3].C0:lerp(CFrame.new(0.5, -0.85 + movement / 15 * math.cos(sine / 4) / 2, (-0.1 - movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			
			elseif walk==false and mode~=25 and mode~="corrupt." and mode~="corrupted." and mode~=5 and mode~="inf" and mode~=13 then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(50*lookvel),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,height+2*cos(sine/37),0)*cf.a(rd(-60+12*sin(sine/37))*lookvel,rd(-10) * rightvel,rd(-20)* rightvel),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(-30),rd(0),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(-20),rd(0),rd(0)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0.5,-0.5)*cf.a(rd(-20),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,1.3,-1)*cf.a(rd(-10),rd(0),rd(0)),Alpha)
			elseif walk==false and mode==5 then			
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(-.7*cos(sine/45),3+.7*sin(sine/45),0)*cf.a(rd(-55-7*sin(sine/35)),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(34+5*sin(sine/23)),rd(0),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(34+30*cos(sine/.5)),rd(30*cos(sine/.5)),rd(30*cos(sine/.5))),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,.4,-0.2)*cf.a(rd(30+7*sin(sine/35)),rd(-10),rd(2)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0.2,-0.4)*cf.a(rd(24+7*sin(sine/35)),rd(10),rd(-2)),Alpha)
			elseif walk==false and mode=="inf" then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(-70),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(4*cos(sine/23/2),5+1.4*cos(sine/23),0)*cf.a(rd(-20+7*sin(sine/23)),rd(90),rd(23*sin(sine/23/2))),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,-0.6+.2*cos(sine/23),-0.7)*cf.a(rd(40+7*sin(sine/23)),rd(30+3*sin(sine/23)),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0.8,-0.1+.2*cos(sine/23),-0.8)*cf.a(rd(93+8*sin(sine/23)),rd(20),rd(90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0-.24*cos(sine/23),-0.4)*cf.a(rd(-23-7*sin(sine/23)),rd(-10),rd(13+3*sin(sine/32))),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0-.24*cos(sine/23),-0.6)*cf.a(rd(-8-7*sin(sine/23)),rd(10),rd(-4)),Alpha)
			elseif walk==false and mode==13 then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,10+5*cos(sine/35),0)*cf.a(rd(0),rd(160*rd(sine/1)),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(90)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(-90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			elseif walk==false and mode=="corrupt." then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,-0.35)*cf.a(rd(-30),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,1+.3*cos(sine/20),0)*cf.a(rd(-5),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/26)),rd(0),rd(-0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/25)),rd(0),rd(0)),Alpha)
			welds[3].C0 = welds[3].C0:Lerp(rightlegc0*CF(-0, 0.6, -0.7) * ANGLES(RAD(-17 + 9.4 * COS(sine / 26)), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), .1)
			welds[5].C0 = welds[5].C0:Lerp(leftlegc0*CF(-0.75, 0,-.5) * ANGLES(RAD(0), RAD(22 + 10.8 * COS(sine / 32)), RAD(80)), .1)
			if math.random(1,42) == 1 then
				welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,-0.35)*cf.a(rd(math.random(-136,136)),rd(math.random(-136,136)),rd(math.random(-136,136))),.5)
				welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/26)+math.random(-136,136)),rd(math.random(-136,136)),rd(-math.random(-136,136))),.5)
				welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/25)+math.random(-136,136)),rd(math.random(-136,136)),rd(math.random(-136,136))),.5)
			end		
     elseif walk==false and mode=="corrupted." then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,-0.35)*cf.a(rd(-30),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,1+.3*cos(sine/20),0)*cf.a(rd(-5),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/26)),rd(0),rd(-0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/25)),rd(0),rd(0)),Alpha)
			welds[3].C0 = welds[3].C0:Lerp(rightlegc0*CF(-0, 0.6, -0.7) * ANGLES(RAD(-17 + 9.4 * COS(sine / 26)), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), .1)
			welds[5].C0 = welds[5].C0:Lerp(leftlegc0*CF(-0.75, 0,-.5) * ANGLES(RAD(0), RAD(22 + 10.8 * COS(sine / 32)), RAD(80)), .1)
			if math.random(1,16) == 1 then
				welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,-0.35)*cf.a(rd(math.random(-136,136)),rd(math.random(-136,136)),rd(math.random(-136,136))),.5)
				welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/26)+math.random(-136,136)),rd(math.random(-136,136)),rd(-math.random(-136,136))),.5)
				welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,-0)*cf.a(rd(20*sin(sine/25)+math.random(-136,136)),rd(math.random(-136,136)),rd(math.random(-136,136))),.5)
			end	
			elseif walk==false and mode==25 then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,3+.8*cos(sine/34),0)*cf.a(rd(70+12*sin(sine/34)),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(-89+38*sin(sine/34/2)),rd(-20),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(-76+35*sin(sine/34/2)),rd(20),rd(0)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(-56*sin(sine/34/2)),rd(-10),rd(10)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(-30*sin(sine/34/2)),rd(10),rd(-10)),Alpha)
			
			end
		end
	end
	if(AnimState=="Jump") then
		local Alpha=.2
		welds[1].C1=welds[1].C1:lerp(cf.n(),.1)
		if attack==false then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,0.1,0)*cf.a(rd(30),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(90)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(-90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0.5,-0.5)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
		end
	end
	if(AnimState=="Fall") then
		local Alpha=.2
		welds[1].C1=welds[1].C1:lerp(cf.n(),.1)
		if attack==false then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,0.1,0)*cf.a(rd(-20),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(90)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(-90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0.5,-0.5)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0.2,-0.2)*cf.a(rd(-10),rd(0),rd(0)),Alpha)
		end
	end
end
